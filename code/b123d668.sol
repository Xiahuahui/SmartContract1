//BCMETH means Blockchain Match Ethereum 
pragma solidity ^0.4.24;

contract BCMETH {
    String currentStatus;
    constructor () public {
        currentStatus="[1]";
    }

    function Ture-ert(String actionStr) public returns(bool){
        if(currentStatus=="[1]" && action=="Ture-ert"){
            currentStatus="[2]";
            return true;
        }
        else
            return false;
    }

    function Timeout-ert(String actionStr) public returns(bool){
        if(currentStatus=="[1]" && action=="Ture-ert"){
            currentStatus="[2]";
            return true;
        }
        else
            return false;
    }

    function Ture-rtewt(String actionStr) public returns(bool){
        if(currentStatus=="[1]" && action=="Ture-ert"){
            currentStatus="[2]";
            return true;
        }
        else
            return false;
    }

    function Timeoutrtewt(String actionStr) public returns(bool){
        if(currentStatus=="[1]" && action=="Ture-ert"){
            currentStatus="[2]";
            return true;
        }
        else
            return false;
    }

}