            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [B]!judge([B],C2.vio)(String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [B]judge([B],C2.vio)(String actionStr) public returns(bool){
