//BCMETH means Blockchain Match Ethereum 
pragma solidity ^0.4.24;

contract BCMETH {
    String currentStatus;
    constructor () public {
        currentStatus="[2, 1, 1, 1, 1]";
    }

    function [A]deposit([A],Contract,10)(String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [A]deposit([A],Contract,10)超时(String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [B](String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [B](String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [B]deliver([B],[A],book)(String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [B]deliver([B],[A],book)超时(String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [A](String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [A](String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [A]===>[C](String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [C]===>[C](String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [A]confirm([A],book)(String actionStr) public returns(bool){
        if(currentStatus=="[2, 1, 1, 1, 1]" && action=="[A]deposit([A],Contract,10)"){
            currentStatus="[3, 1, 1, 1, 1]";
            return true;
        }
        else
            return false;
    }

    function [A]confirm([A],book)超时(String actionStr) public returns(bool){
